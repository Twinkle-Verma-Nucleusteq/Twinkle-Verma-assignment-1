<!DOCTYPE module PUBLIC "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN" "https://checkstyle.org/dtds/configuration_1_3.dtd">

<module name="Checker">
    <module name="TreeWalker">
        <!-- Require JavaDoc comments for all classes, methods, fields, and parameters -->
        <module name="JavadocType" />
        <module name="JavadocMethod" />
        <module name="JavadocField" />
        <module name="JavadocParameter" />
    </module>
    <module name="SuppressionFilter">
        <property name="checks" value="FinalParameters" />
        <property name="files" value=".*" />
        <module name="AllowElement" />
    </module>
</module>
